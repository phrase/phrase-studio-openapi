                      "description": "Recordings within the project"
                    }
                  },
                  "required": ["id", "name", "status", "createdAt", "updatedAt"]
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized (invalid API key)"
          },
          "404": {
            "description": "Project not found"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      },
      "post": {
        "summary": "Finalize Project Uploads",
        "description": "Finalize uploaded files and start processing the project",
        "operationId": "finalizeProjectUploads",
        "security": [{ "apiKeyAuth": [] }],
        "parameters": [
          {
            "name": "projectId",
            "in": "path",
            "required": true,
            "description": "Project ID",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "uploadedFiles": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "fileId": {
                          "type": "string",
                          "description": "File ID from upload response"
                        },
                        "filename": {
                          "type": "string",
                          "description": "Original filename"
                        },
                        "contentType": {
                          "type": "string",
