{
  "openapi": "3.0.3",
  "info": {
    "title": "Studio API",
    "description": "API for creating transcription, translation, and dubbing projects with file URLs.",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://api.studio.us.phrase.com",
      "description": "Production US Environment"
    },
    {
      "url": "https://api.studio.eu.phrase.com",
      "description": "Production EU Environment"
    },
    {
      "url": "https://api.studio.us.phrase-staging.com",
      "description": "Stage US Environment"
    }
  ],
  "paths": {
    "/v1/projects": {
      "get": {
        "summary": "List Projects",
        "description": "Retrieve a list of projects for the authenticated account",
        "operationId": "listProjects",
        "security": [{ "apiKeyAuth": [] }],
        "responses": {
          "200": {
            "description": "Projects retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string",
                            "description": "Project ID",
                            "example": "01jzmvc20cjq5hj5saa96ttp2x"
                          },
                          "name": {
                            "type": "string",
                            "description": "Project name",
                            "example": "Meeting Recording"
                          },
                          "status": {
                            "type": "string",
                            "description": "Current project status",
                            "enum": ["pending", "processing", "completed", "failed"],
                            "example": "processing"
                          },
                          "createdAt": {
                            "type": "string",
                            "format": "date-time",
                            "description": "Project creation timestamp"
                          },
                          "updatedAt": {
                            "type": "string",
                            "format": "date-time",
                            "description": "Project last update timestamp"
                          },
                          "recordings": {
                            "type": "array",
                            "items": {
                              "type": "object",
                              "properties": {
                                "id": {
                                  "type": "string",
                                  "description": "Recording ID"
                                },
                                "name": {
                                  "type": "string",
                                  "description": "Recording name"
                                },
                                "type": {
                                  "type": "string",
                                  "description": "File type"
                                },
                                "extension": {
                                  "type": "string",
                                  "description": "File extension"
                                },
                                "sourceLanguage": {
                                  "type": "string",
                                  "description": "Source language code"
                                },
                                "translationLanguages": {
                                  "type": "array",
                                  "items": {
                                    "type": "string"
                                  },
                                  "description": "Target translation languages"
                                },
                                "dubbingLanguages": {
                                  "type": "array",
                                  "items": {
                                    "type": "string"
                                  },
                                  "description": "Target dubbing languages"
                                },
                                "summaryEnabled": {
                                  "type": "boolean",
                                  "description": "Whether summary is enabled"
                                },
                                "customAttributesEnabled": {
                                  "type": "boolean",
                                  "description": "Whether custom attributes are enabled"
                                },
                                "duration": {
                                  "type": "number",
                                  "description": "Recording duration in seconds"
                                },
                                "createdAt": {
                                  "type": "string",
                                  "format": "date-time",
                                  "description": "Recording creation timestamp"
                                },
                                "updatedAt": {
                                  "type": "string",
                                  "format": "date-time",
                                  "description": "Recording last update timestamp"
                                },
                                "status": {
                                  "type": "string",
                                  "description": "Recording status",
                                  "enum": ["pending", "processing", "completed", "failed"]
                                }
                              }
                            },
                            "description": "Recordings within the project"
                          }
                        },
                        "required": ["id", "name", "status", "createdAt", "updatedAt"]
                      },
                      "description": "Array of projects"
                    },
                    "total": {
                      "type": "number",
                      "description": "Total number of projects",
                      "example": 25
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized (invalid API key)"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      },
      "post": {
        "summary": "Create Project",
        "description": "The Create Project API allows you to programmatically create transcription, translation, and dubbing projects using file URLs or file uploads. The API handles file processing, cost calculation, and project creation automatically.",
        "operationId": "createProject",
        "security": [{ "apiKeyAuth": [] }],
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "files": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "binary"
                    },
                    "minItems": 1,
                    "maxItems": 5,
                    "description": "Files to process (up to 5 files)"
                  },
                  "title": {
                    "type": "string",
                    "description": "Project title",
                    "example": "Meeting Recording"
                  },
                  "sourceLanguage": {
                    "type": "string",
                    "description": "Source language code. Use \"auto\" to auto-detect source language.",
                    "default": "auto",
                    "example": "en",
                    "enum": ["auto", "en", "es", "fr", "de", "it", "pt", "ru", "ja", "ko", "zh-cn", "tr", "pl", "nl", "ar", "sv", "fi", "da", "no", "cs", "el", "hu", "ro", "bg", "hr", "sk", "sl", "et", "lv", "lt", "mt", "ga", "cy", "eu", "ca", "gl", "oc", "br", "co", "fy", "gd", "gv", "kw", "lb", "li", "nds", "pap", "pms", "rm", "sc", "scn", "sco", "szl", "vec", "vls", "wa", "wo", "zea"]
                  },
                  "subtitleProfiles": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "subtitleProfileId": {
                          "type": "string",
                          "format": "uuid",
                          "description": "UUID of the subtitle profile to use"
                        },
                        "languageCode": {
                          "type": "string",
                          "description": "Language code for the subtitle profile",
                          "enum": ["auto", "en", "es", "fr", "de", "it", "pt", "ru", "ja", "ko", "zh-cn", "tr", "pl", "nl", "ar", "sv", "fi", "da", "no", "cs", "el", "hu", "ro", "bg", "hr", "sk", "sl", "et", "lv", "lt", "mt", "ga", "cy", "eu", "ca", "gl", "oc", "br", "co", "fy", "gd", "gv", "kw", "lb", "li", "nds", "pap", "pms", "rm", "sc", "scn", "sco", "szl", "vec", "vls", "wa", "wo", "zea"]
                        }
                      },
                      "required": ["subtitleProfileId", "languageCode"]
                    },
                    "description": "Subtitle profiles to generate for the project"
                  },
                  "translationLanguages": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "enum": ["en", "es", "fr", "de", "it", "pt", "ru", "ja", "ko", "zh-cn", "tr", "pl", "nl", "ar", "sv", "fi", "da", "no", "cs", "el", "hu", "ro", "bg", "hr", "sk", "sl", "et", "lv", "lt", "mt", "ga", "cy", "eu", "ca", "gl", "oc", "br", "co", "fy", "gd", "gv", "kw", "lb", "li", "nds", "pap", "pms", "rm", "sc", "scn", "sco", "szl", "vec", "vls", "wa", "wo", "zea"]
                    },
                    "description": "Target translation language codes"
                  },
                  "dubbingLanguages": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "enum": ["en", "en-US", "en-GB", "ja", "zh-cn", "de", "hi", "fr", "fr-CA", "ko", "pt-BR", "pt", "it", "es", "es-MX", "id", "nl", "tr", "fil", "pl", "sv", "bg", "ro", "ar-SA", "ar", "cs", "el", "fi", "hr", "ms", "sk", "da", "ta", "uk", "ru", "es-MX", "es-AR"]
                    },
                    "description": "Dubbing languages. Must also be included in translationLanguages to ensure translation before dubbing."
                  }
                },
                "required": ["title", "subtitleProfiles"]
              }
            },
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "title": {
                    "type": "string",
                    "description": "Project title",
                    "example": "Meeting Recording"
                  },
                  "files": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "url": {
                          "type": "string",
                          "format": "uri",
                          "description": "Publicly accessible file URL"
                        },
                        "filename": {
                          "type": "string",
                          "description": "Optional filename override"
                        }
                      },
                      "required": ["url"]
                    },
                    "minItems": 1,
                    "maxItems": 5,
                    "description": "Array of files to process"
                  },
                  "sourceLanguage": {
                    "type": "string",
                    "description": "Source language code. Use \"auto\" to auto-detect source language.",
                    "default": "auto",
                    "example": "en",
                    "enum": ["auto", "en", "es", "fr", "de", "it", "pt", "ru", "ja", "ko", "zh-cn", "tr", "pl", "nl", "ar", "sv", "fi", "da", "no", "cs", "el", "hu", "ro", "bg", "hr", "sk", "sl", "et", "lv", "lt", "mt", "ga", "cy", "eu", "ca", "gl", "oc", "br", "co", "fy", "gd", "gv", "kw", "lb", "li", "nds", "pap", "pms", "rm", "sc", "scn", "sco", "szl", "vec", "vls", "wa", "wo", "zea"]
                  },
                  "subtitleProfiles": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "subtitleProfileId": {
                          "type": "string",
                          "format": "uuid",
                          "description": "UUID of the subtitle profile to use"
                        },
                        "languageCode": {
                          "type": "string",
                          "description": "Language code for the subtitle profile",
                          "enum": ["auto", "en", "es", "fr", "de", "it", "pt", "ru", "ja", "ko", "zh-cn", "tr", "pl", "nl", "ar", "sv", "fi", "da", "no", "cs", "el", "hu", "ro", "bg", "hr", "sk", "sl", "et", "lv", "lt", "mt", "ga", "cy", "eu", "ca", "gl", "oc", "br", "co", "fy", "gd", "gv", "kw", "lb", "li", "nds", "pap", "pms", "rm", "sc", "scn", "sco", "szl", "vec", "vls", "wa", "wo", "zea"]
                        }
                      },
                      "required": ["subtitleProfileId", "languageCode"]
                    },
                    "description": "Subtitle profiles to generate for the project"
                  },
                  "translationLanguages": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "enum": ["en", "es", "fr", "de", "it", "pt", "ru", "ja", "ko", "zh-cn", "tr", "pl", "nl", "ar", "sv", "fi", "da", "no", "cs", "el", "hu", "ro", "bg", "hr", "sk", "sl", "et", "lv", "lt", "mt", "ga", "cy", "eu", "ca", "gl", "oc", "br", "co", "fy", "gd", "gv", "kw", "lb", "li", "nds", "pap", "pms", "rm", "sc", "scn", "sco", "szl", "vec", "vls", "wa", "wo", "zea"]
                    },
                    "description": "Target translation language codes"
                  },
                  "dubbingLanguages": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "enum": ["en", "en-US", "en-GB", "ja", "zh-cn", "de", "hi", "fr", "fr-CA", "ko", "pt-BR", "pt", "it", "es", "es-MX", "id", "nl", "tr", "fil", "pl", "sv", "bg", "ro", "ar-SA", "ar", "cs", "el", "fi", "hr", "ms", "sk", "da", "ta", "uk", "ru", "es-MX", "es-AR"]
                    },
                    "description": "Dubbing languages. Must also be included in translationLanguages to ensure translation before dubbing."
                  }
                },
                "required": ["title", "files", "subtitleProfiles"]
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Project created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string",
                      "description": "Created project ID",
                      "example": "01jzmvc20cjq5hj5saa96ttp2x"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request (invalid input)",
            "content": {
              "application/json": {
                "examples": {
                  "no_files": {
                    "summary": "No files provided",
                    "value": {
                      "statusCode": 400,
                      "message": "There are no files"
                    }
                  },
                  "too_many_files": {
                    "summary": "Too many files",
                    "value": {
                      "statusCode": 400,
                      "message": "Maximum 5 files allowed. You currently have 7 file(s)."
                    }
                  },
                  "invalid_format": {
                    "summary": "Unsupported file format",
                    "value": {
                      "statusCode": 400,
                      "message": "Unsupported file format. Only MP3, WAV, M4A, MP4, and MOV files are allowed."
                    }
                  },
                  "too_large": {
                    "summary": "File too large",
                    "value": {
                      "statusCode": 400,
                      "message": "File size cannot exceed 5GB."
                    }
                  },
                  "too_long": {
                    "summary": "File too long",
                    "value": {
                      "statusCode": 400,
                      "message": "File duration cannot exceed 180 minutes."
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized (invalid API key)"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      },
      "post": {
        "summary": "Create Project with Upload",
        "description": "Create a project for file uploads. Returns signed URLs for uploading files to cloud storage.",
        "operationId": "createProjectWithUpload",
        "security": [{ "apiKeyAuth": [] }],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "title": {
                    "type": "string",
                    "description": "Project title",
                    "example": "Meeting Recording"
                  },
                  "sourceLanguage": {
                    "type": "string",
                    "description": "Source language code. Use \"auto\" to auto-detect source language.",
                    "default": "auto",
                    "example": "en",
                    "enum": ["auto", "en", "es", "fr", "de", "it", "pt", "ru", "ja", "ko", "zh-cn", "tr", "pl", "nl", "ar", "sv", "fi", "da", "no", "cs", "el", "hu", "ro", "bg", "hr", "sk", "sl", "et", "lv", "lt", "mt", "ga", "cy", "eu", "ca", "gl", "oc", "br", "co", "fy", "gd", "gv", "kw", "lb", "li", "nds", "pap", "pms", "rm", "sc", "scn", "sco", "szl", "vec", "vls", "wa", "wo", "zea"]
                  },
                  "subtitleProfiles": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "subtitleProfileId": {
                          "type": "string",
                          "format": "uuid",
                          "description": "UUID of the subtitle profile to use"
                        },
                        "languageCode": {
                          "type": "string",
                          "description": "Language code for the subtitle profile",
                          "enum": ["auto", "en", "es", "fr", "de", "it", "pt", "ru", "ja", "ko", "zh-cn", "tr", "pl", "nl", "ar", "sv", "fi", "da", "no", "cs", "el", "hu", "ro", "bg", "hr", "sk", "sl", "et", "lv", "lt", "mt", "ga", "cy", "eu", "ca", "gl", "oc", "br", "co", "fy", "gd", "gv", "kw", "lb", "li", "nds", "pap", "pms", "rm", "sc", "scn", "sco", "szl", "vec", "vls", "wa", "wo", "zea"]
                        }
                      },
                      "required": ["subtitleProfileId", "languageCode"]
                    },
                    "description": "Subtitle profiles to generate for the project"
                  },
                  "translationLanguages": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "enum": ["en", "es", "fr", "de", "it", "pt", "ru", "ja", "ko", "zh-cn", "tr", "pl", "nl", "ar", "sv", "fi", "da", "no", "cs", "el", "hu", "ro", "bg", "hr", "sk", "sl", "et", "lv", "lt", "mt", "ga", "cy", "eu", "ca", "gl", "oc", "br", "co", "fy", "gd", "gv", "kw", "lb", "li", "nds", "pap", "pms", "rm", "sc", "scn", "sco", "szl", "vec", "vls", "wa", "wo", "zea"]
                    },
                    "description": "Target translation language codes"
                  },
                  "dubbingLanguages": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "enum": ["en", "en-US", "en-GB", "ja", "zh-cn", "de", "hi", "fr", "fr-CA", "ko", "pt-BR", "pt", "it", "es", "es-MX", "id", "nl", "tr", "fil", "pl", "sv", "bg", "ro", "ar-SA", "ar", "cs", "el", "fi", "hr", "ms", "sk", "da", "ta", "uk", "ru", "es-MX", "es-AR"]
                    },
                    "description": "Dubbing languages. Must also be included in translationLanguages to ensure translation before dubbing."
                  },
                  "fileCount": {
                    "type": "number",
                    "minimum": 1,
                    "maximum": 5,
                    "description": "Number of files to be uploaded",
                    "example": 2
                  }
                },
                "required": ["title", "subtitleProfiles", "fileCount"]
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Project created successfully with upload URLs",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string",
                      "description": "Created project ID",
                      "example": "01jzmvc20cjq5hj5saa96ttp2x"
                    },
                    "uploadUrls": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "fileId": {
                            "type": "string",
                            "description": "Unique file identifier"
                          },
                          "signedUrl": {
                            "type": "string",
                            "description": "Signed URL for file upload"
                          },
                          "path": {
                            "type": "string",
                            "description": "File path in storage"
                          },
                          "contentType": {
                            "type": "string",
                            "description": "Expected content type"
                          }
                        },
                        "required": ["fileId", "signedUrl", "path"]
                      },
                      "description": "Array of upload URLs for files"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request (invalid input)",
            "content": {
              "application/json": {
                "examples": {
                  "invalid_file_count": {
                    "summary": "Invalid file count",
                    "value": {
                      "statusCode": 400,
                      "message": "File count must be between 1 and 5"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized (invalid API key)"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/v1/projects/{id}": {
      "get": {
        "summary": "Get Project Details",
        "description": "Retrieve detailed information about a specific project",
        "operationId": "getProjectDetails",
        "security": [{ "apiKeyAuth": [] }],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "Project ID",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Project details retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string",
                      "description": "Project ID",
                      "example": "01jzmvc20cjq5hj5saa96ttp2x"
